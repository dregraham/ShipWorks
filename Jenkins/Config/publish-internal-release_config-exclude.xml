<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>Pushes the debug installer to a central location on the network</description>
  <logRotator class="hudson.tasks.LogRotator">
    <daysToKeep>21</daysToKeep>
    <numToKeep>5</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <se.diabol.jenkins.pipeline.PipelineProperty plugin="delivery-pipeline-plugin@0.7.4">
      <taskName>Publish installer</taskName>
      <stageName>Stage 3: Publish Pre-alpha</stageName>
    </se.diabol.jenkins.pipeline.PipelineProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>true</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>C:\jenkins-builds\@@BRANCH_NAME@@</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command># Copy the debug installer to the pre-alpha testing directory for @@FEATURE_NAME@@
if [ ! -d &quot;//intfs01/support/Testing/InternalReleases/&quot; ];
then 
     mkdir //intfs01/support/Testing/InternalReleases/
fi

if [ ! -d &quot;//intfs01/support/Testing/InternalReleases/@@FEATURE_NAME@@/&quot; ];
then 
     mkdir //intfs01/support/Testing/InternalReleases/@@FEATURE_NAME@@/
fi

cp ./Artifacts/Distribute/*.* //intfs01/support/Testing/InternalReleases/@@FEATURE_NAME@@/</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.logparser.LogParserPublisher plugin="log-parser@1.0.8">
      <unstableOnWarning>false</unstableOnWarning>
      <failBuildOnError>false</failBuildOnError>
      <parsingRulesPath>C:\jenkins-builds\ShipWorksParseRules.txt</parsingRulesPath>
    </hudson.plugins.logparser.LogParserPublisher>
    <hudson.tasks.Mailer plugin="mailer@1.8">
      <recipients>t.hughes@shipworks.com k.croke@shipworks.com a.benz@shipworks.com m.mulaosmanovic@shipworks.com c.hicks@shipworks.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
	<hudson.plugins.growl.GrowlPublisher plugin="growl@1.1">
      <IP>10.1.10.121,10.1.10.109</IP>
    </hudson.plugins.growl.GrowlPublisher>
  </publishers>
  <buildWrappers/>
</project>