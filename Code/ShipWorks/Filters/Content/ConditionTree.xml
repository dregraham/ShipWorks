      <Condition identifier="OrderItem.Code" displayAs="Code"></Condition>
      <Condition identifier="OrderItem.Description" displayAs="Description"></Condition>
      <Condition identifier="OrderItem.ISBN" displayAs="ISBN"></Condition>
      <Condition identifier="OrderItem.Location" displayAs="Location"></Condition>
      <Condition identifier="OrderItem.Quantity" displayAs="Quantity"></Condition>
      <Condition identifier="OrderItem.UnitCost" displayAs="Unit Cost"></Condition>
      <Condition identifier="OrderItem.UnitPrice" displayAs="Unit Price"></Condition>
      <Condition identifier="OrderItem.UnitWeight" displayAs="Unit Weight"></Condition>
      <Condition identifier="OrderItem.UPC" displayAs="UPC"></Condition>
      <Condition identifier="OrderItem.LocalStatus" displayAs="Local Status"></Condition>
      <Separator></Separator>
      <Condition identifier="BigCommerceOrderItem.IsDigitalItem"></Condition>
      <Condition identifier="BigCommerceOrderItem.EventDate"></Condition>
      <Condition identifier="BigCommerceOrderItem.EventName"></Condition>
      <Condition identifier="BuyDotComOrderItem.ListingID"></Condition>
      <Condition identifier="ChannelAdvisorOrderItem.SiteName"></Condition>
      <Condition identifier="ChannelAdvisorOrderItem.BuyerID"></Condition>
      <Condition identifier="ChannelAdvisorOrderItem.SalesSourceID"></Condition>
      <Condition identifier="ChannelAdvisorOrderItem.DistributionCenter"></Condition>
      <Condition identifier="ChannelAdvisorOrderItem.IsFBA"></Condition>
      <Condition identifier="EbayOrderItem.SellingManagerRecord"></Condition>
      <Condition identifier="EbayOrderItem.Feedback"></Condition>
      <Condition identifier="EbayOrderItem.PaymentMethod"></Condition>
      <Condition identifier="EbayOrderItem.CheckoutStatus"></Condition>
      <Condition identifier="EbayOrderItem.ShippedStatus"></Condition>
      <Condition identifier="EbayOrderItem.PaidStatus"></Condition>
      <Condition identifier="EbayOrderItem.PayPalAddressStatus"></Condition>
