<Page
    x:Class="ShipWorks.Installer.Views.SystemCheck"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ShipWorks.Installer.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="System Check"
    d:DesignHeight="300"
    d:DesignWidth="300"
    DataContext="{Binding Path=SystemCheckViewModel, Source={StaticResource Locator}}"
    mc:Ignorable="d">
    <Grid>
        <TextBlock Text="Checking your system..." HorizontalAlignment="Center"/>
    </Grid>
</Page>