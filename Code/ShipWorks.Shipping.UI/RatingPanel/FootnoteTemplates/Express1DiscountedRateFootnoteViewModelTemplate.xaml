<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ui="http://schemas.shipworks.com/ui">
    <DataTemplate x:Key="Express1DiscountedRateFootnoteViewModelTemplate">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
            <Image Source="/Shipworks.Core;component/Resources/check2.png"
                    Width="16"
                    Height="16"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center" />
            <TextBlock Margin="6,1,0,0">
                This is your Express1 discounted rate.
                <Hyperlink Command="{Binding ViewSavings}">View Savings...</Hyperlink>
            </TextBlock>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>