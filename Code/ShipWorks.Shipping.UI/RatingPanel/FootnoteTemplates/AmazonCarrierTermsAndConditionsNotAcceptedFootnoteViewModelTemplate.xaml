<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ui="http://schemas.shipworks.com/ui">
    <DataTemplate x:Key="AmazonCarrierTermsAndConditionsNotAcceptedFootnoteViewModelTemplate">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
            <Image  Style="{DynamicResource Image.Icon}"
                    Source="/Shipworks.Core;component/Resources/information2.png" />
            <TextBlock Margin="6,1,0,0">
                Additional rates are available. For more info,
                <Hyperlink Command="{Binding ShowDialog}">click here</Hyperlink>.
            </TextBlock>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>